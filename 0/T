{
    Manikin
    {
        type            zeroGradient;
    }
    Internal_Wall
    {
        type            zeroGradient;
    }
    Inlet
    {
        type            fixedValue;
        value           uniform 293;
    }
    Outlet
    {
        type            zeroGradient;
    }
    Exposed_Wall
    {
        type            fixedValue;
        value           uniform 283;
    }
}


